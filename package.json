{
  "name": "hasoop",
  "version": "0.1.1",
  "main": "dist/index.js",
  "license": "MIT",
  "keywords": [
    "sqoop",
    "node",
    "client",
    "restful"
  ],
  "description": "a simple node.js client for Sqoop 2",
  "author": {
    "name": "Madadata",
    "email": "hi@madadata.com",
    "url": "https://www.madadata.com/"
  },
  "repository": {
    "type": "git",
    "url": "Madadata/hasoop"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf .nyc_output coverage dist",
    "prebuild": "npm run lint",
    "build": "babel -d dist src",
    "lint": "standard --verbose | snazzy",
    "test": "npm run pre-test-mysql && mocha",
    "precommit": "npm run lint",
    "prepush": "npm run lint",
    "prepublish": "npm run clean && npm run build && npm run lint",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "pre-test-mysql": "docker exec -i test-mysql mysql -uroot -p1234 < test/init.sql"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "husky": "^0.12.0",
    "mocha": "^3.2.0",
    "nyc": "^10.0.0",
    "preset": "^1.0.0",
    "rimraf": "^2.5.4",
    "snazzy": "^5.0.0",
    "standard": "^8.6.0"
  }
}
